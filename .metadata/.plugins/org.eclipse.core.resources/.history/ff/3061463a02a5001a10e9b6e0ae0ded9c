package com.capgemini.lastMile.entity;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

/**
 * @author Sowrasree Banerjee
 *
 */


@Entity
@Table(name="CAFEDETAILS")
public class CafeDetails {
	
	 @Id
	 @GeneratedValue(strategy= GenerationType.IDENTITY)
	 private Long cafe_id;
	  
	private String cafe_name;
	 
	 private String cafe_loaction;
	 
	 private String cafe_owner;
	 
	 private String cafe_rating;
	 
	 private String cafe_image_path;
	 
	 
	 public Long getCafe_id() {
		return cafe_id;
	}

	public void setCafe_id(Long cafe_id) {
		this.cafe_id = cafe_id;
	}


	public String getCafe_name() {
		return cafe_name;
	}

	public void setCafe_name(String cafe_name) {
		this.cafe_name = cafe_name;
	}

	public String getCafe_loaction() {
		return cafe_loaction;
	}

	public void setCafe_loaction(String cafe_loaction) {
		this.cafe_loaction = cafe_loaction;
	}

	public String getCafe_owner() {
		return cafe_owner;
	}

	public void setCafe_owner(String cafe_owner) {
		this.cafe_owner = cafe_owner;
	}

	public String getCafe_rating() {
		return cafe_rating;
	}

	public void setCafe_rating(String cafe_rating) {
		this.cafe_rating = cafe_rating;
	}

	public String getCafe_image_path() {
		return cafe_image_path;
	}

	public void setCafe_image_path(String cafe_image_path) {
		this.cafe_image_path = cafe_image_path;
	}
	 

}
