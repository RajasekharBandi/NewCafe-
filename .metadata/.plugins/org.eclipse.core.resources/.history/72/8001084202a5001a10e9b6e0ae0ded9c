package com.capgemini.lastMile.entity;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.Table;
 
/**
 * @author Sowrasree Banerjee
 *
 */



@Entity
@Table(name="TICKET")
public class Ticket{
  
 @Id
 @GeneratedValue(strategy= GenerationType.IDENTITY)
 private Long ticket_number;
  

 @ManyToOne
 @JoinColumn(name = "order_id")
 private Orders orders;
 

private String status;
 
 private String ticket_date;
 
 private String ticket_location;

public Long getTicket_number() {
	return ticket_number;
}


public void setTicket_number(Long ticket_number) {
	this.ticket_number = ticket_number;
}

public Orders getOrders() {
	return orders;
}

public void setOrders(Orders orders) {
	this.orders = orders;
}

public String getStatus() {
	return status;
}

public void setStatus(String status) {
	this.status = status;
}

public String getDate() {
	return ticket_date;
}

public void setDate(String date) {
	this.ticket_date = date;
}

public String getLocation() {
	return ticket_location;
}

public void setLocation(String location) {
	this.ticket_location = location;
}
 
 
 
}

  

 
 